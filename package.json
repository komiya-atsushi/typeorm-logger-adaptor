{
  "name": "typeorm-logger-adaptor",
  "version": "1.1.0",
  "description": "Logger adaptors for TypeORM",
  "author": {
    "name": "KOMIYA Atsushi",
    "url": "https://github.com/komiya-atsushi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/komiya-atsushi/typeorm-logger-adaptor.git"
  },
  "bugs": {
    "url": "https://github.com/komiya-atsushi/typeorm-logger-adaptor/issues"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "compile": "tsc --noEmit",
    "lint": "gts lint",
    "test": "docker compose up -d && jest --coverage --collect-coverage-from='src/**/*.ts'",
    "test:ci": "docker compose up -d && jest --ci",
    "clean": "rm -rf dist build coverage reports",
    "fix": "gts fix",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "keywords": [
    "typescript",
    "typeorm",
    "logger",
    "winston",
    "bunyan"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.5",
    "bunyan": "^1.8.5",
    "eslint-plugin-import": "^2.31.0",
    "gts": "^6.0.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-mock-extended": "^3.0.7",
    "mysql2": "^3.11.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.0",
    "typescript": "^5.6.3",
    "winston": "^3.0.0"
  },
  "peerDependencies": {
    "bunyan": "^1.8.5",
    "typeorm": ">=0.2.0 <0.4.0",
    "winston": "^3.0.0"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./logger/bunyan": "./dist/logger/bunyan/index.js",
    "./logger/winston": "./dist/logger/winston/index.js",
    "./bunyan": "./dist/logger/bunyan/index.js",
    "./winston": "./dist/logger/winston/index.js"
  },
  "typesVersions": {
    "*": {
      "logger/bunyan": [
        "dist/logger/bunyan/"
      ],
      "logger/winston": [
        "dist/logger/winston/"
      ],
      "bunyan": [
        "dist/logger/bunyan/"
      ],
      "winston": [
        "dist/logger/winston/"
      ]
    }
  }
}
